{
    "name": "saturn-fm",
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-istanbul": "^5.1.0",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-preset-airbnb": "^3.0.1",
        "clean-webpack-plugin": "^0.1.17",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "eslint": "^5.8.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-react": "^7.10.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^2.0.0",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^3.2.0",
        "istanbul-instrumenter-loader": "^3.0.0",
        "mini-css-extract-plugin": "^0.4.4",
        "node-sass": "^4.9.3",
        "nyc": "^13.1.0",
        "prettier": "^1.14.2",
        "sass-loader": "^7.1.0",
        "sinon": "^7.1.0",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.1",
        "webpack": "^4.23.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-glsl-loader": "^1.0.1",
        "webpack-manifest-plugin": "^2.0.4",
        "webpack-merge": "^4.1.4",
        "webpack-node-externals": "^1.6.0"
    },
    "scripts": {
        "build": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
        "build:analyze": "NODE_ENV=production webpack --config config/webpack.config.analyze.js",
        "deploy": "NODE_ENV=production webpack --config config/webpack.config.prod.js; gh-pages -d dist",
        "lint": "eslint ./src ./config",
        "lint:fix": "eslint --fix ./src ./config",
        "start": "NODE_ENV=development webpack-dev-server --hot --inline --config config/webpack.config.dev.js",
        "test:coverage": "NODE_ENV=coverage nyc --reporter=lcov --reporter=text --webpack-config config/webpack.config.coverage.js npm run test",
        "prettier": "prettier --single-quote --trailing-comma es5 --write './**/*.js'"
    },
    "dependencies": {
        "@tweenjs/tween.js": "^17.2.0",
        "babel-polyfill": "^6.26.0",
        "prop-types": "^15.6.2",
        "react": "^16.8.0-alpha.0",
        "react-dom": "^16.8.0-alpha.0",
        "react-redux": "^5.0.6",
        "redux": "^4.0.1",
        "three": "^0.97.0"
    },
    "nyc": {
        "include": [
            "src/**/*.js"
        ],
        "temp-directory": "./coverage",
        "instrument": false,
        "sourceMap": false
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ]
}
