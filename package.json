    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "es6-tween": "^5.5.11",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "jsmediatags": "^3.9.5",
    "next": "13.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.0",
    "three": "^0.140.2",
    "typescript": "4.9.5",
    "uuid": "^8.2.0"
  }
