{
    "name": "saturn-player",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.5",
        "babel-plugin-module-resolver": "^2.7.1",
        "babel-preset-airbnb": "^2.4.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "chai": "^4.1.2",
        "clean-webpack-plugin": "^0.1.17",
        "copy-webpack-plugin": "^4.2.0",
        "css-loader": "^0.28.7",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.2",
        "eslint": "^4.10.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-chai-friendly": "^0.4.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "fetch-mock": "^5.13.1",
        "file-loader": "^1.1.5",
        "gh-pages": "^1.1.0",
        "html-webpack-plugin": "^2.30.1",
        "istanbul-instrumenter-loader": "^3.0.0",
        "jsdom": "11.3.0",
        "jsdom-global": "3.0.2",
        "mocha": "^4.0.1",
        "mocha-webpack": "^1.0.1",
        "node-sass": "^4.5.3",
        "nyc": "^11.2.1",
        "prettier": "^1.8.2",
        "redux-devtools": "^3.4.0",
        "redux-mock-store": "^1.3.0",
        "sass-loader": "^6.0.6",
        "sinon": "^4.1.2",
        "sinon-chai": "^2.14.0",
        "style-loader": "^0.19.0",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "webpack": "^3.7.1",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-server": "^2.9.3",
        "webpack-glsl-loader": "^1.0.1",
        "webpack-manifest-plugin": "^1.3.2",
        "webpack-merge": "^4.1.0",
        "webpack-node-externals": "^1.6.0"
    },
    "scripts": {
        "build": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
        "build:analyze": "NODE_ENV=production webpack --config config/webpack.config.analyze.js",
        "deploy": "NODE_ENV=production webpack --config config/webpack.config.prod.js gh-pages -d dist",
        "lint": "eslint ./src ./test ./config",
        "lint:fix": "eslint --fix ./src ./test ./config",
        "start": "webpack-dev-server --hot --inline --config config/webpack.config.dev.js",
        "test": "HONEYBADGER_REPORT_DATA=false mocha-webpack --recursive --include test/setup.test.js  --webpack-config config/webpack.config.test.js",
        "test:watch": "HONEYBADGER_REPORT_DATA=false mocha-webpack --watch --recursive --include test/setup.test.js --webpack-config config/webpack.config.test.js",
        "test:coverage": "NODE_ENV=coverage HONEYBADGER_REPORT_DATA=false nyc --reporter=lcov --reporter=text --webpack-config config/webpack.config.coverage.js npm run test",
        "prettier": "prettier --single-quote --trailing-comma es5 --write './**/*.js'"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "expo-three": "^2.2.2-alpha.1",
        "history": "^4.7.2",
        "moment": "^2.19.4",
        "orbit-controls-es6": "^1.0.10",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-logger": "^3.0.6",
        "redux-promise-middleware": "^5.0.0",
        "three": "^0.90.0"
    },
    "nyc": {
        "include": [
            "src/**/*.js"
        ],
        "temp-directory": "./coverage",
        "instrument": false,
        "sourceMap": false
    }
}
