{
  "name": "saturn-fm",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src ./config",
    "lint:fix": "eslint --fix ./src ./config",
    "prettier": "prettier --write './**/*.js'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{json,css}": [
