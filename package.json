{
  "name": "saturn-fm",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "build:analyze": "NODE_ENV=production webpack --config config/webpack.config.analyze.js",
    "deploy": "NODE_ENV=production webpack --config config/webpack.config.prod.js; gh-pages -d dist",
    "lint": "eslint ./src ./config",
    "lint:fix": "eslint --fix ./src ./config",
    "start": "NODE_ENV=development webpack-dev-server --mode development --inline --config config/webpack.config.dev.js --hot --open ",
    "test:coverage": "NODE_ENV=coverage nyc --reporter=lcov --reporter=text --webpack-config config/webpack.config.coverage.js npm run test",
    "prettier": "prettier --write './**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "@sentry/browser": "^5.18.1",
    "core-js": "^3.6.5",
    "es6-tween": "^5.5.11",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jsmediatags": "^3.9.3",
    "lint-staged": "^10.2.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "three": "^0.118.3",
    "three-gltf-loader": "^1.111.0",
    "uuid": "^8.2.0"
  }
}
